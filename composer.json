{
  "name": "bartfeenstra/payment-symfony-http-foundation-bridge",
  "description": "Integrates bartfeenstra/payment with symfony/http-foundation.",
  "license": "MIT",
  "authors": [
    {
      "name": "Bart Feenstra",
      "email": "bart@mynameisbart.com"
    }
  ],
  "require": {
    "bartfeenstra/payment": "0.2.*",
    "php": "~5.5 || ~7.0",
    "symfony/http-foundation": "~2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "BartFeenstra\\Payment\\Bridge\\Symfony\\HttpFoundation\\": "src"
    }
